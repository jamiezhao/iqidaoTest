<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Default suite" parallel="tests" thread-count="1">
     <listeners>
       <listener class-name="iqidaoTest.Utils.OverrideIAnnotationTransformer" />
       <listener class-name="org.uncommons.reportng.HTMLReporter" />
       <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
     </listeners>
  <test verbose="2" preserve-order="true" name="PublicTest">
    <classes>
      <class name="iqidaoTest.testCase.PublicNewsTC" />
    </classes>
  </test> 
  <test verbose="2" preserve-order="true" name="quizmenu">  
    <classes>
      <class name="iqidaoTest.testCase.QuizTC" />
    </classes>
  </test> 
  <test verbose="2" preserve-order="true" name="acitivity">
	     <groups>
          <dependencies>
              <group name="EditCombo" depends-on="CreateSeason"/> 
              <group name="Creaentrancetetest" depends-on="EditCombo"/>   
